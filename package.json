{
  "name": "react-tape",
  "version": "4.0.0",
  "description": "A boilerplate for React using Tape for testing.",
  "author": "Fredrik Carlsson <me@fc.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fc-io/react-tape"
