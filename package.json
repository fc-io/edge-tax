    "enzyme": "^2.4.1",
    "eslint": "^3.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.2.2",
    "html-webpack-plugin": "^2.22.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
